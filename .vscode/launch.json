    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'stelae'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=stelae"
                ],
                "filter": {
                    "name": "stelae",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'stelae'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=stelae",
                    "--package=stelae"
                ],
                "filter": {
                    "name": "stelae",
                    "kind": "bin"
                }
            },
            "args": ["-a", "C:/OLL/oll-test-repos", "serve"],
            "cwd": "${workspaceFolder}",
